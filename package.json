{
  "name": "ngx-components",
  "version": "1.0.0",
  "description": "Useful Ngx Components",
  "keywords": [],
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .tmp && rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "./node_modules/uglify-js/bin/uglifyjs dist/bundles/ngx-components.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ngx-components.umd.min.js",
    "copy": "cpx './package-dist.json' dist && renamer --find 'package-dist.json' --replace 'package.json' ./dist/*",
    "build": "npm run clean && npm run transpile && npm run package && npm run minify && npm run copy"
  },
  "license": "MIT",
  "homepage": "github.com/ChrisTheButcher/",
  "repository": {
    "type": "git",
    "url": "github.com/ChrisTheButcher/"
  },
  "devDependencies": {
    "@angular/compiler": "^4.4.6",
    "@angular/compiler-cli": "^4.4.6",
    "cpx": "^1.5.0",
    "renamer": "^0.6.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "typescript": "^2.5.3",
    "uglify-js": "^3.1.5"
  },
  "dependencies": {
    "@angular/common": "^4.4.6",
    "@angular/core": "^4.4.6",
    "rxjs": "^5.5.1",
    "zone.js": "^0.8.18"
  }
}
